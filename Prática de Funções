void main ()
{
  duplo mediaFinal = 0;
  
  if (mediaFinal> = 9.0) {
    print ("Conceito final: A");
  }
    else if (mediaFinal> = 7.0 && mediaFinal <= 8.99) {
      print ("Conceito final: B");
    }
  else if (mediaFinal> = 5.0 && mediaFinal <= 6.99) {
      impressão ("Conceito final: C");
    }
  else if (mediaFinal> = 0,0 && mediaFinal <= 4,9) {
      print ("Conceito final: D");
    }
  
}

