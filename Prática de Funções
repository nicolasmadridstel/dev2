// 11


void main ()
{
  
  duplo num1 = 6;
  print ("Primeiro número: $ num1");
  duplo num2 = 6;
  print ("Segundo número: $ num2");
  
  
  para (var i = num1; i <= num2; i ++)
  {
    imprimir (i);
  }
    para (var i = num1; i> = num2; i--)
  {
    imprimir (i);
  }
  if (num1 == num2)
  {
    print ("Valores iguais!");
  }
}
  

