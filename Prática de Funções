// 10

void main ()
{
  
  duplo num1 = -2;
  print ("Primeiro número: $ num1");
  duplo num2 = 6;
  print ("Segundo número: $ num2");
  
  soma duplo = (num1 + num2);
  print ("A soma dos números é: $ soma");
  
  duplo produto = (num1 * (num2 * num2));
  print ("O primeiro número pelo quadrado do segundo é: $ produto");
  
  qprimeiro duplo = (num1 * num1);
  print ("O quadrado do primeiro número é: $ qprimeiro");
  
  triplo duplo = ((soma * 2) * 3);
  print ("O triplo do dobro da soma dos dois números é: $ triplo");
  
  duplo metdobro = (triplo / 2);
  print ("A metade do triplo do dobro da soma dos dois números é: $ metdobro");
  
  if (num1 <0) {
    imprimir (- (num1));
  }
  senão{
    imprimir (num1);
  }
   
}

