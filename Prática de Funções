  }
    else if (idade> = 14 && age <= 17) {
      imprimir ("Cotegoria: Juvenil B");
    }
   else if (idade> = 11 && age <= 13) {
      imprimir ("Cotegoria: Juvenil A");
    }
   else if (idade> = 8 && age <= 10) {
