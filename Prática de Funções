 // 1

  void main ()
 { 
    double nota1;
    double nota2;
    double nota3;
    
    String a = "a";
    String b = "b";
    String c = "c";
    
    switch (c) {
      case "a": print ("A média aritmética é: $ {(nota1 + nota2 + nota3) / 3}");
        pausa;
      case "b": print ("A média ponderada é: $ {((nota1 * 5 + nota2 * 3 nota3 * 2) / 10)}");
        pausa;
      case "c": print ("A média harmônica é: $ {(3) / ((1 / nota1) + (1 / nota2) + (1 / nota3))}");
        padrão: imprimir ("erro");
      
    }
        
  }

