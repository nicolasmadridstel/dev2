// 12

void main ()
{
  
  duplo num1 = 4;
  print ("Primeiro número: $ num1");
  duplo num2 = 8;
  print ("Segundo número: $ num2");
  
  String operacao = "-";
  
  calcular dinâmico () {
    
    switch (operação) {
      
      caso '*':
      imprimir (num1 * num2);
      pausa;
      caso '/':
      imprimir (num1 / num2);
      pausa;
      case '+':
      imprimir (num1 + num2);
      pausa;
      caso '-':
      imprimir (num1 - num2);
      pausa;
      
    }
          
  }
  
  calcular ();
    
}
