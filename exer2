void  main ()
{
  // --- 1 ---
  print ( '--- 1 --- \ n ' );
  imprimir ( 'Este laço é o para! \ n ' );
  para ( int i =  0 ; i <=  100 ; i ++ )
  {
    imprimir (i);
  }
}

