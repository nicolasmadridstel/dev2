void  main ()
{
  duplo numA =  4 ;
  duplo numB =  2 ;
  var multiplicacao = numA * numB;
  contador duplo =  0 ;
  Faz
  {
    imprimir (contador);
    contador + = multiplicacao;
  } while (contador <=  1200 );
}
